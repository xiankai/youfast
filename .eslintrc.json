{
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "react",
        "react-native"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "react-native/react-native": true
    },
    "rules": {
        "prettier/prettier": 2,
        "no-undef": 2
    }
}
